{
  "title": "SHIFT/CMD Key Swap",
  "rules": [
    {
      "description": "swap left shift/command keys for vncviewer",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_shift"
          },
          "to": {
            "key_code": "left_command"
          }
        },
        {
          "type": "basic",
          "from": {
            "key_code": "left_command"
          },
          "to": {
            "key_code": "left_shift"
          }
        }
      ]
    }
  ]
}
