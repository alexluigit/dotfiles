#!/bin/sh
xrdb -merge /etc/X11/xinit/xresources &
mouse-gestures "BT5.1 Mouse" &
fcitx5 &
# export XDG_CURRENT_DESKTOP=KDE
# export GTK_IM_MODULE=ibus QT_IM_MODULE=ibus XMODIFIERS=@im=ibus
# GDK_SCALE=1 GDK_DPI_SCALE=1.35 GTK_THEME=aibus ibus-daemon -drxR &
flameshot &
autolock -b /usb/devices/1-2 -d DP-0 &
redshift -b 1.0:0.7 -l 30.59:114.27 &
picom --experimental-backends &
xsetroot -cursor_name left_ptr &

exec awesome
