#!/bin/sh
easystroke --config-dir $XDG_CONFIG_HOME/easystroke &
remaps &
xrdb -merge $XDG_CONFIG_HOME/X11/xresources
sxhkd &
aria2c --input-file=/home/alex/.cache/aria2/aria2.session &
deadd-notification-center &
privoxy --no-daemon ~/.config/privoxy/config &
fcitx &
flameshot &
autolock &
redshift &
xsetroot -cursor_name left_ptr &
picom --xrender-sync-fence --experimental-backends &
xwallpaper --zoom ~/Pictures/Wallpaper/Dany-0.png &
launchPolybar &

exec bspwm
